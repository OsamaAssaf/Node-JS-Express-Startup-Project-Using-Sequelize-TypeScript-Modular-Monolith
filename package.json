    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migrate": "typeorm migration:run -- -d data-source.ts"
  },
  "repository": {
    "type": "git",
